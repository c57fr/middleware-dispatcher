{
  "name": "procurios/middleware-dispatcher",
  "description": "Simple PSR-15 compliant middleware dispatcher.",
  "license": "MIT",
  "keywords": [
    "middleware",
    "dispatcher",
    "http",
    "psr-15",
    "psr-7"
  ],
  "require": {
    "roave/security-advisories": "dev-master",
    "php": ">=7.0",
    "psr/http-message": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Procurios\\Http\\MiddlewareDispatcher\\": "src/",
      "Psr\\Http\\Middleware\\": "psr/http-middleware/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload-dev": {
    "psr-4": {
      "Procurios\\Http\\MiddlewareDispatcher\\test\\": "test/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "provide": {
    "psr/http-middleware": "dev-develop"
  }
}
